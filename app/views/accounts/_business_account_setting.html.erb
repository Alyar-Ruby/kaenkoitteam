<script>
var prev_elm_url="";
var field_name = ""
  $(function() {
    $(".edit-field").click(function() {
      elem = $(this)
      prev_elm_url = $(this).prev().data('url');
      field_name = $(this).prev().attr('id')
      field_value = $(this).prev().html();
      $(this).prev().html('<input type="text" value='+field_value+'><a  onclick="update_field(this)">Update</a><a class="cancel-field" onclick=cancel_field(this)>Cancel</a>')
      $(this).addClass("slided-in");
    })
    
  })

  function update_field(obj) {
    data_params = field_name+"="+$(obj).prev().val()
    $.ajax({
      url: prev_elm_url,
      data: data_params,
      method: "put",
      success: function(data) {
        value = $(obj).prev().val();    
        $(obj).parent().parent().find(".edit-field").show();
        $(obj).parent().html(value); 
      },
      error: function(data) {
        $(obj).parent().parent().find(".edit-field").show();
        $(obj).parent().html("Unable to update")    
      }

    })
    
  }
  function cancel_field(obj, value) {
    value = $(obj).prev().prev().val();
    $(obj).parent().parent().find(".edit-field").show();
    $(obj).parent().html(value)
  }
</script>


