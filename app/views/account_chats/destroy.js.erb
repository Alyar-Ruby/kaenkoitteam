<%if @account.present? %>
  $("#chatbox-prototype<%=@account.id%>").hide();
  <% align = 0 %>
  <%if @available_chat_box.present? %>
    <% @available_chat_box.each do |box| %>
      <%if align == 0 %>
       $("#chatbox-prototype<%=box%>").removeAttr('style').css("right", "320px;")
      <% else %>
      <% width = (align)*(290)+320 %>
        $("#chatbox-prototype<%=box%>").removeAttr('style').css('right', '<%=width%>px')
      <% end %>
      <% align = align + 1 %>
    <% end %>
    <% end %>
<% else %>

<% end %>
$(".ajax-loading").hide();




  